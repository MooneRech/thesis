#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/node_modules:/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/node_modules:/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules/.pnpm/babylon@6.18.0/node_modules:/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules/.pnpm/node_modules:/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules:/mnt/c/Users/Inga/IdeaProjects/node_modules:/mnt/c/Users/Inga/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules/babylon/bin/node_modules:/mnt/c/Users/Inga/IdeaProjects/example-app/node_modules/babylon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../babylon/bin/babylon.js" "$@"
else
  exec node  "$basedir/../babylon/bin/babylon.js" "$@"
fi
